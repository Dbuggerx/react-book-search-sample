{
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileTransformer.js",
    "\\.(css|scss)$": "identity-obj-proxy"
  },
  "collectCoverageFrom": ["**/*.{ts,tsx}", "!**/node_modules/**"],
  "rootDir": "../..",
  "roots": ["<rootDir>/src/"],
  "testRegex": "(_test|_spec|\\.test|\\.spec)\\.tsx?$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
  "moduleDirectories": ["node_modules"],
  "testURL": "http://localhost",
  "transform": {
    "^.+\\.tsx?$": "<rootDir>/config/jest/babelJestCustomTransformer.js"
  }
}
